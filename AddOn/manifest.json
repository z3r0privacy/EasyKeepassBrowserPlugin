{
    "manifest_version": 2,
    "name": "EasyKeepassBrowserPlugin",
    "version": "0.1",
    "description": "A simple Plugin trying to automatically fill in your passwords",
    "author": "Michael Roth",
    "icons": {
        "48": "icon_closed.svg",
        "96": "icon_closed.svg"
    },
    "content_scripts": [
        {
            "matches": ["<all_urls>"],
            "run_at": "document_end",
            "js": ["helper.js", "security.js", "ekbp.js"]
        }
    ],
    "permissions": [
        "<all_urls>",
        "activeTab",
        "webRequest"
    ],
    "browser_action":{
        "default_icon": "icon_closed.svg",
        "default_title": "Easy Keepass Passwords",
        "default_popup": "addonWindow.html"
    },
    "browser_specific_settings": {
        "gecko": {
            "id":"{eb1edb6d-64b2-4c6e-8934-becabf9d8e35}"
        }
    }
}